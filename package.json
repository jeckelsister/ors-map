    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
